{
  "name": "dev-test-node",
  "version": "1.0.0",
  "description": "Nodejs dev test",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "start-inspect": "nodemon --exec 'node --inspect -r ts-node/register src/index.ts'",
    "build": "tsc",
    "go": "ts-node src/index.ts",
    "go:prod": "node ./build/src/index.js",
    "test-watch": "jest --watchAll",
    "test": "jest"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/hapi__joi": "^17.1.3",
    "@types/jest": "^24.0.13",
    "@types/redis": "^2.8.25",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "jest": "^24.8.0",
    "mocha": "^8.0.1",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.1.7",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.5"
  }

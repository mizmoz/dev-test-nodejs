{
  "name": "dev-test-node",
  "version": "1.0.0",
  "description": "Nodejs dev test",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "start-inspect": "nodemon --exec 'node --inspect -r ts-node/register src/index.ts'",
    "build": "tsc",
    "go": "ts-node src/index.ts",
    "go:prod": "node ./build/src/index.js",
    "test-watch": "jest --watchAll",
    "test": "jest",
    "start-docker": "yarn install && ts-node scripts/initJwt.ts && ts-node scripts/initRedis.ts && yarn start",
    "init:redis": "ts-node scripts/initRedis.ts",
    "init:jwt": "ts-node scripts/initJwt.ts"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/redis": "^2.8.28",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^9.1.1",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.5"
  }
}

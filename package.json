{
  "name": "dev-test-node",
  "version": "1.0.0",
  "description": "Nodejs dev test",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "seed": "ts-node src/test/seed/dev.ts",
    "start": "nodemon",
    "start-inspect": "nodemon --exec 'node --inspect -r ts-node/register src/index.ts'",
    "build": "tsc",
    "go": "ts-node src/index.ts",
    "go:prod": "node ./build/src/index.js",
    "test-watch": "jest --watchAll",
    "test": "jest"
  },
  "dependencies": {
    "@koa/router": "^10.0.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa__router": "^8.0.4",
    "@types/node": "^12.0.2",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.11",
    "jest": "^26.6.3",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^8.1.0",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.2.4"
  }
}

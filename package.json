  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/jest": "^24.0.13",
    "@types/morgan": "^1.9.1",
    "@types/node": "^12.12.54",
    "@types/redis": "^2.8.27",
    "@types/supertest": "^2.0.10",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^8.0.3",

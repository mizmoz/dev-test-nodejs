    "jsonwebtoken": "^8.5.1",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.21",
    "@types/node": "^12.0.2",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^8.1.0",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",

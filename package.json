{
  "name": "dev-test-node",
  "version": "1.0.0",
  "description": "Nodejs dev test",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "start-inspect": "nodemon --exec 'node --inspect -r ts-node/register src/index.ts'",
    "start-redis-service": "node src/redisService.js",
    "build": "tsc",
    "go": "ts-node src/index.ts",
    "go:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "go:prod": "node ./build/src/index.js",
    "test-watch": "jest --watchAll",
    "test": "jest"
  },
  "dependencies": {
    "express-session": "^1.17.2",
    "http": "0.0.1-security",
    "path": "^0.12.7",
    "redis": "^3.1.2",
    "redis-server": "^1.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/redis": "^2.8.29",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.1.6",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.5"
  }
}

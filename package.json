{
   "name": "dev-test-node",
   "version": "1.0.0",
   "description": "Nodejs dev test",
   "main": "src/index.ts",
   "private": true,
   "scripts": {
      "start": "nodemon",
      "start-inspect": "nodemon --exec 'node --inspect -r ts-node/register src/index.ts'",
      "build": "tsc",
      "go": "ts-node src/index.ts",
      "go:prod": "node ./build/index.js",
      "test-watch": "jest --watchAll",
      "test": "jest",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
   },
   "dependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.19.0",
      "@types/cors": "^2.8.6",
      "@types/helmet": "^0.0.47",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/redis": "^2.8.25",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.19.0",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^3.23.3",
      "jsonwebtoken": "^8.5.1",
      "mysql": "^2.18.1",
      "redis": "^3.0.2",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.25"
   },
   "devDependencies": {
      "@types/jest": "^26.0.5",
      "@types/node": "^8.0.29",
      "jest": "^26.1.0",
      "nodemon": "^2.0.4",
      "prettier": "^1.17.1",
      "ts-jest": "^26.1.3",
      "ts-node": "3.3.0",
      "typescript": "^3.9.7"
   }
}
